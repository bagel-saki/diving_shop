<%
let leftContent = [
    {main: 'キャンペーン', first: 'ライセンス取得', second: '貸切体験ダイビング', third: 'ナイトダイビング', link: 'archive-campaign'},
    {main: '私たちについて', first: '', second: '', third: '', link: 'page-about'},
    {main: 'ダイビング情報', first: 'ライセンス講習', second: '体験ダイビング', third: 'ファンダイビング', link: 'page-diving-info'},
    {main: 'ブログ', first: '', second: '', third: '', link: 'home'},
]

let rightContent = [
    {main: 'お客様の声', first: '', second: '', third: '', link: 'archive-voices'},
    {main: '料金一覧', first: 'ライセンス講習', second: '体験ダイビング', third: 'ファンダイビング', link: 'page-price'},
    {main: 'よくある質問', first: '', second: '', third: '', link: 'page-faq'},
    {main: 'プライバシー<br>ポリシー', first: '', second: '', third: '', link: 'page-privacy-policy'},
    {main: '利用規約', first: '', second: '', third: '', link: 'page-terms'},
    {main: 'お問い合わせ', first: '', second: '', third: '', link: 'page-contact'},
]
%>

            <nav class="p-drawer js-drawer">
                <div class="l-inner">
                    <div class="p-drawer__container">
                        <ul class="p-drawer__leftItems"> <% leftContent.forEach(function(title) { %>
                        <li class="p-drawer__item">
                            <a class="p-drawer__title" href="<%- title.link %>.html"><%- title.main %></a>
                                <% if (title.first || title.second || title.third) { %>
                            <div class="p-drawer__lists">
                                <a class="p-drawer__list" href=""><%- title.first %></a>
                                <a class="p-drawer__list" href=""><%- title.second %></a>
                                <a class="p-drawer__list" href=""><%- title.third %></a>
                            </div>
                            <% } %>
                        </li>
                        <% }); %>
                        </ul>
                        <ul class="p-drawer__rightItems"> <% rightContent.forEach(function(title) { %>
                            <li class="p-drawer__item">
                                <a class="p-drawer__title" href="<%- title.link %>.html"><%- title.main %></a>
                                <% if (title.first || title.second || title.third) { %>
                                <div class="p-drawer__lists">
                                    <a class="p-drawer__list" href=""><%- title.first %></a>
                                    <a class="p-drawer__list" href=""><%- title.second %></a>
                                    <a class="p-drawer__list" href=""><%- title.third %></a>
                                </div>
                                <% } %>
                            </li>
                        <% }); %>
                        </ul>
                    </div>
                </div>
            </nav>