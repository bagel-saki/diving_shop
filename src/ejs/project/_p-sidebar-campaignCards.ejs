<%
let campaignCard = [
  {image:'1', alt:'赤、青、黄等の小さな魚が海の中を泳いでいる', title:'ライセンス取得', text:'全部コミコミ(お一人様)' , originalPrice:'￥56,000', newPrice:'￥46,000'},
  {image:'2', alt:'海岸を出発し島に向かう二艘の船', title:'貸切体験ダイビング', text:'全部コミコミ(お一人様)' , originalPrice:'￥20,000', newPrice:'￥16,000'}
]
%>

<ul class="p-sidebar-campaignCards">
    <% campaignCard.forEach(function(side) { %>
        <li class="p-sidebar-campaignCards__item">
        <%- include(ejsPath + './project/_p-sidebar-campaignCard', {
            image: side.image,
            alt: side.alt,
            title: side.title,
            text: side.text,
            originalPrice: side.originalPrice,
            newPrice: side.newPrice,
            }) %>
        </li>
        <% }); %>
</ul>