<% baseUrlPath='.' ; ejsPath='.' ; pageData="top" %>
<!DOCTYPE html>
<html lang="ja">
    <%- include(ejsPath + '/common/_head' ,{ baseUrl: baseUrlPath, page:
        json[pageData] }) %>
  <style>

body{
      /* background: pink;  */
    }
    .wrapper {
      max-width: 1080px;
      width: 100%;
      margin: auto;
      /* background: pink; */


    }

    .h2 {
      margin-top: 75px;
      background-color: yellow;
      color: black;
    }

    .p {
      margin-top: 10px;
      margin-bottom: 60px;
      color: white;
    }

    </style>
  <body>
    <div class="wrapper">
      <!-- prettier-ignore -->
      <h2 class="h2">.p-header</h2>
      <p class="p">ヘッダーとドロワー</p>
      <%- include(ejsPath + '/project/_p-header' )%>

    <h2 class="h2">.p-lower-informationContents</h2>
    <p class="p">ダイビング情報ページのコンテンツ群</p>
    <%- include(ejsPath + '/project/_p-lower-informationContents' ) %>  

      <h2 class="h2">.c-starfish</h2>
