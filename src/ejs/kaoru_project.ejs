    <%- include(ejsPath + '/project/_p-lower-singleBlog' ) %>  
    
    <h2 class="h2">.p-information-section</h2>
    <p class="p">トップページのダイビング情報セクション</p>
    <%- include(ejsPath + '/project/_p-information-section' ) %>   
    
    <h2 class="h2">.p-information-card</h2>
    <p class="p">ダイビング情報カード</p> 



    <h2 class="h2">.p-contents</h2>
    <p class="p">コンテンツ（フッター・サイトマップ）</p>
    <%- include(ejsPath + '/project/_p-contents', { section: 'footer', color: 'white' }) %>
    <%- include(ejsPath + '/project/_p-contents', { section: 'sitemap', color: 'black' }) %>

    <h2 class="h2">.p-sitemap.ejs</h2>
