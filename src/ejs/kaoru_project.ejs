      <%- include(ejsPath + '/project/_p-contact-form') %> 

      
      
    </div>
    <h2 class="h2">.p-lower-top</h2>
    <p class="p">下層ページトップ</p>
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'campaign', first: 'campaign', middle:'', last:'',}) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'aboutus', first: 'about us', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'information', first: 'information', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'blog', first: 'blog', middle:'', last:'', }) %>
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'voice', first: 'voice', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'price', first: 'price', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'faq', first: 'faq',  middle:'', last:'',}) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'contact', first: 'contact', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'privacy', first: 'privacy policy', middle:'', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'sitemap', first: 'site', middle:' map', last:'', }) %> 
    <%- include(ejsPath + '/project/_p-lower-mv' ,{lower:'terms', first: 'terms', middle:' of', last:' service', }) %> 
    
    
    <h2 class="h2">.p-blog-section</h2>
    <p class="p">ブログセクション</p>
    <%- include(ejsPath + '/project/_p-blog-section' ) %> 
    
